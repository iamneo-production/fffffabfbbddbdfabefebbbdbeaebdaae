<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>WithdrawServlet</servlet-name>
  	<display-name>WithdrawServlet</display-name>
  	<description></description>
  	<servlet-class>controller.WithdrawServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>BalanceServlet</servlet-name>
  	<display-name>BalanceServlet</display-name>
  	<description></description>
  	<servlet-class>controller.BalanceServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DepositServlet</servlet-name>
  	<display-name>DepositServlet</display-name>
  	<description></description>
  	<servlet-class>controller.DepositServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>default</servlet-name>
  	<url-pattern>*.css</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>default</servlet-name>
  	<url-pattern>*.svg</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>WithdrawServlet</servlet-name>
  	<url-pattern>/withdraw</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BalanceServlet</servlet-name>
  	<url-pattern>/balance</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DepositServlet</servlet-name>
  	<url-pattern>/deposit</url-pattern>
  </servlet-mapping>
</web-app>